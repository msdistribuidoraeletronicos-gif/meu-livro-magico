{
  "functions": {
    "api/[...all].js": {
      "includeFiles": [
        "app.js",
        "admin.page.js",
        "generate.page.js",
        "books/**",
        "public/**",
        "landing.html",
        "how-it-works.html",
        "exemplos.html"
      ]
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/[...all].js" }
  ]
}